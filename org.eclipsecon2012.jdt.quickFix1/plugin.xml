<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension point="org.eclipse.jdt.ui.quickFixProcessors">
        <quickFixProcessor class="org.eclipsecon2012.jdt.quickfix1.processor.QuickFixNullCheck" id="org.eclipsecon2012.jdt.quickFix1.quickFixProcessor1">
            <handledMarkerTypes>
                <markerType
                      id="org.eclipsecon2012.jdt.quickFix1.mymarker">
                </markerType>
            </handledMarkerTypes>
        </quickFixProcessor>
    </extension>
    <extension id="mymarker" name="My Problem" point="org.eclipse.core.resources.markers">
        <super type="org.eclipse.jdt.core.problem" />
        <super type="org.eclipse.core.resources.problemmarker" />
        <super type="org.eclipse.core.resources.textmarker" />
        <persistent value="true" />
    </extension>
    <extension
          point="org.eclipse.ui.commands">
       <category
             id="org.eclipsecon2012.jdt.quickFix1.commands.category"
             name="Sample Category">
       </category>
       <command
             categoryId="org.eclipsecon2012.jdt.quickFix1.commands.category"
             id="org.eclipsecon2012.jdt.quickFix1.commands.sampleCommand"
             name="Sample Command">
       </command>
    </extension>
    <extension
          point="org.eclipse.ui.handlers">
       <handler
             class="org.eclipsecon2012.jdt.quickfix1.handlers.MarkerGenerator"
             commandId="org.eclipsecon2012.jdt.quickFix1.commands.sampleCommand">
       </handler>
    </extension>
    <extension
          point="org.eclipse.ui.bindings">
       <key
             commandId="org.eclipsecon2012.jdt.quickFix1.commands.sampleCommand"
             contextId="org.eclipse.ui.contexts.window"
             schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
             sequence="M1+6">
       </key>
    </extension>
    <extension
          point="org.eclipse.ui.menus">
       <menuContribution
             locationURI="menu:org.eclipse.ui.main.menu?after=additions">
          <menu
                id="org.eclipsecon2012.jdt.quickFix1.menus.sampleMenu"
                label="Sample Menu"
                mnemonic="M">
             <command
                   commandId="org.eclipsecon2012.jdt.quickFix1.commands.sampleCommand"
                   id="org.eclipsecon2012.jdt.quickFix1.menus.sampleCommand"
                   mnemonic="S">
             </command>
          </menu>
       </menuContribution>
       <menuContribution
             locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
          <toolbar
                id="org.eclipsecon2012.jdt.quickFix1.toolbars.sampleToolbar">
             <command
                   commandId="org.eclipsecon2012.jdt.quickFix1.commands.sampleCommand"
                   icon="icons/sample.png"
                   id="org.eclipsecon2012.jdt.quickFix1.toolbars.sampleCommand"
                   tooltip="Say hello world">
             </command>
          </toolbar>
       </menuContribution>
    </extension>
</plugin>
